            </Grid>
            <Grid Grid.Column="2">
                <TextBox  Name="TextBox_L1_D"  Text="120" BorderBrush="White" FontSize="14"  FontWeight="SemiBold" TextAlignment="Center" VerticalAlignment="Center" Foreground="White" Background="Black" Height="20"  Width="60"/>
            </Grid>
            <Grid Grid.Column="3">
                <TextBox  Name="TextBox_L1_I" Text="400" BorderBrush="White" FontSize="14"  FontWeight="SemiBold" TextAlignment="Center" VerticalAlignment="Center" Foreground="White" Background="Black" Height="20"  Width="60"/>
            </Grid>
            <Grid Grid.Column="4">
                <TextBox  Name="TextBox_L1_Dist" Text="400" BorderBrush="White" FontSize="14"  FontWeight="SemiBold" TextAlignment="Center" VerticalAlignment="Center" Foreground="White" Background="Black" Height="20"  Width="60"/>
            </Grid>
        </Grid>
        <Grid Grid.Row="20">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="100*"/>
                <ColumnDefinition Width="100*"/>
                <ColumnDefinition Width="100*"/>
                <ColumnDefinition Width="100*"/>
                <ColumnDefinition Width="100*"/>
            </Grid.ColumnDefinitions>
            <Grid Grid.Column="3">
                <Button x:Name="BTN_GET_FW_PID" Content="{DynamicResource TITLE_READ}" BorderThickness="2" BorderBrush="White" Background="#00DDDDDD" FontSize="14" FontWeight="SemiBold" Foreground="White"  Height="25"  Width="80" Click="BTN_GET_FW_PID_Click">
                    <Button.Template>
                        <ControlTemplate TargetType="{x:Type Button}">
                            <Border BorderBrush="{TemplateBinding Control.BorderBrush}" BorderThickness="1" CornerRadius="5,5,5,5" Name="PART_Background">
                                <ContentPresenter Content="{TemplateBinding ContentControl.Content}" HorizontalAlignment="Center" VerticalAlignment="Center" />
                            </Border>
                            <ControlTemplate.Triggers>
                                <Trigger Property="UIElement.IsMouseOver" Value="True">
                                    <Setter Property="Border.Background" TargetName="PART_Background">
                                        <Setter.Value>
                                            <LinearGradientBrush EndPoint="0,1" StartPoint="0,0">
                                                <GradientStop Color="Gray" Offset="0.5" />
                                            </LinearGradientBrush>
                                        </Setter.Value>
                                    </Setter>
                                </Trigger>
                            </ControlTemplate.Triggers>
                        </ControlTemplate>
                    </Button.Template>
                </Button>
            </Grid>
            <Grid Grid.Column="4">
                <Button x:Name="BTN_SET_FW_PID" Content="{DynamicResource TITLE_SET}" BorderThickness="2" BorderBrush="White" Background="#00DDDDDD" FontSize="14" FontWeight="SemiBold" Foreground="White" Height="25"  Width="80" Click="BTN_SET_FW_PID_Click">
                    <Button.Template>
                        <ControlTemplate TargetType="{x:Type Button}">
                            <Border BorderBrush="{TemplateBinding Control.BorderBrush}" BorderThickness="1" CornerRadius="5,5,5,5" Name="PART_Background">
                                <ContentPresenter Content="{TemplateBinding ContentControl.Content}" HorizontalAlignment="Center" VerticalAlignment="Center" />
                            </Border>
                            <ControlTemplate.Triggers>
                                <Trigger Property="UIElement.IsMouseOver" Value="True">
                                    <Setter Property="Border.Background" TargetName="PART_Background">
                                        <Setter.Value>
                                            <LinearGradientBrush EndPoint="0,1" StartPoint="0,0">
                                                <GradientStop Color="Gray" Offset="0.5" />
                                            </LinearGradientBrush>
                                        </Setter.Value>
                                    </Setter>
                                </Trigger>
                            </ControlTemplate.Triggers>
                        </ControlTemplate>
                    </Button.Template>
                </Button>
            </Grid>
        </Grid>
    </Grid>
</Page>
