<Window x:Class="VIKGroundStation.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="{DynamicResource TITLE_COMPANY_NAME}" Height="800" Width="1366"  WindowState="Maximized" Loaded="Main_Window_Loaded"   WindowStyle="None" Background="Black" MouseDown="Window_MouseDown" Icon="VK_DESKTOP.ico" >
