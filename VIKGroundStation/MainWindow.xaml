                <ColumnDefinition Width="15*"/>
                <ColumnDefinition Width="30*"/>
                <ColumnDefinition Width="4*"/>
                <ColumnDefinition Width="4*"/>
                <ColumnDefinition Width="4*"/>
                <ColumnDefinition Width="4*"/>
            </Grid.ColumnDefinitions>
            <!-- 机型设置 -->
            <Grid x:Name="MENUE_PLANE_SETTING" Grid.Column="0"  >
                <Button x:Name="MENUE_BTN_PLANE_SETTINGS"   Content="{DynamicResource TITLE_MENUE_FLY_SETTINGS}"   FontSize="15" FontWeight="SemiBold" Foreground="White"  Background="Black" Click="MENUE_BTN_PLANE_SETTINGS_Click"  VerticalAlignment="Center"/>
            </Grid>
            <!-- 飞行检测 -->
            <Grid x:Name="MENUE_FLY_INFO" Grid.Column="1">
                <Button x:Name="MENUE_BTN_FLY_INFO" Content="{DynamicResource TITLE_MENUE_FLY_INFOR}"  FontSize="15" FontWeight="SemiBold" Foreground="White" Background="Black" Click="MENUE_BTN_FLY_INFO_Click"  VerticalAlignment="Center"/>
            </Grid>
            <!--任务设置-->
            <Grid x:Name="MENUE_TASK_SETTING" Grid.Column="2">
                <Button x:Name="MENUE_BTN_TASK_SETTING" Content="{DynamicResource TITLE_MENUE_TASK_FUNCTION}"  FontSize="15" FontWeight="SemiBold" Foreground="White" Background="Black" Click="MENUE_BTN_TASK_SETTING_Click"  VerticalAlignment="Center"/>
            </Grid>
            <!--地图切换-->
            <Grid x:Name="MENUE_2D_3D" Grid.Column="3">
                <Button x:Name="MENUE_BTN_2D_3D" Content="2D"  FontSize="15" FontWeight="SemiBold" Foreground="White" Background="Black" Click="MENUE_BTN_2D_3D_Click"  VerticalAlignment="Center"/>
            </Grid>
            <!-- 航线设置 -->
            <Grid x:Name="MENUE_WPT_PLAN" Grid.Column="4">
                <Button x:Name="MENUE_BTN_WPT_PLAN" Content="{DynamicResource TITLE_MENUE_WPT_PLAN}"   FontSize="15" FontWeight="SemiBold" Foreground="White"  Background="Black" Click="MENUE_BTN_WPT_PLAN_Click" VerticalAlignment="Center"/>
            </Grid>
            <!-- 通信设置 -->
            <Grid x:Name="MENUE_LINK_SET" Grid.Column="5">
                <Button x:Name="MENUE_BTN_LINK_SET" Content="{DynamicResource TITLE_MENUE_LINK_SET}"  FontSize="15" FontWeight="SemiBold" Foreground="White" Background="Black" Click="MENUE_BTN_LINK_SET_Click"  VerticalAlignment="Center"/>
            </Grid>
            <Grid Grid.Column="6" >
                <ComboBox Name="combox_language"   HorizontalAlignment="Center" VerticalAlignment="Center" Height="25" Width="100" FontSize="15"  FontWeight="SemiBold" Focusable="False" SelectionChanged="combox_language_Changed"/>
            </Grid>
            <Grid Grid.Column="8" >
